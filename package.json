{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "this is a react boilerplate with typescript, eslint and jest",
  "main": "index.tsx",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server --mode development --config config/webpack.common.config.js --open",
    "build:dev": "cross-env NODE_ENV=development webpack --env.ENVIRONMENT=development --mode development --config config/webpack.common.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --env.ENVIRONMENT=production --mode production --config config/webpack.common.config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
